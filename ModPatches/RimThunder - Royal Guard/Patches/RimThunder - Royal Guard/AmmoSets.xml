<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<!-- RT_Churchill_Vehicle -->
	<Operation Class="XmlExtensions.PatchOperationSafeAdd">
		<compare>InnerText</compare>
		<xpath>Defs/Vehicles.VehicleDef[defName="RT_Churchill_Vehicle"]</xpath>
		<value>
			<descriptionHyperlinks>
				<!-- RT_Churchill_Tank_Turret -->
				<CombatExtended.AmmoSetDef>AmmoSet_75x350mmR</CombatExtended.AmmoSetDef>
				<!--RT_BESA_MGTurret-->
				<CombatExtended.AmmoSetDef>AmmoSet_792x57mmMauser</CombatExtended.AmmoSetDef>
				<!--RT_GrenadeLauncherGB-->
				<CombatExtended.AmmoSetDef>RTC_AmmoSet_SmokeGrenade</CombatExtended.AmmoSetDef>
				<!--RT_AVRE_Turret-->
